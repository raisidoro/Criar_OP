{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python: Arquivo Atual",
			"type": "python",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal",
			"justMyCode": true
		},
		{
			"type": "totvs_language_debug",
